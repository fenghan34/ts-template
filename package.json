    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "unbuild": "^0.7.2"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue}": "eslint --fix"
