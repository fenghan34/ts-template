  "scripts": {
    "dev": "unbuild --stub",
    "build": "rimraf dist && unbuild",
    "lint": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "prepare": "pnpx husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@fenghan/eslint-config": "^0.1.2",
    "@types/jest": "^27.4.1",
    "eslint": "^8.11.0",
