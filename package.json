{
  "name": "ts-template",
  "version": "0.1.0",
  "packageManager": "pnpm@6.32.4",
  "description": "",
  "keywords": [],
  "license": "MIT",
  "author": "Feng Han <fenghan770@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fenghan34/ts-template.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
