{
  "compilerOptions": {
    "lib": ["ESNEXT", "DOM"],
    "target": "ES2017",
    "moduleResolution": "node",
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "strict": true,
    "noEmit": true
  }
}
